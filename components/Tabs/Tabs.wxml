<view class="tabs">
  <view class="tabs_title">
    <view bindtap="handleTitle" data-item="{{index}}" class="title_item {{currentIndex==index ?'tabActive':''}}" wx:for="{{tabs}}" wx:key="id">
      {{item.value}}
    </view>
  </view>
  <view class="tabs_content">
    <slot></slot>
  </view>
</view>
